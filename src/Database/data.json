[
  {
    "id": "prueba",
    "version": 1,
    "fields": {
      "field_1": {
        "type": "text",
        "label": "<p>Nombre completo</p>",
        "required": true,
        "extras": {
          "validation_rules": ["LETERS_ONLY"],
          "div_class": "clase-custom label-custom"
        }
      },
      "field_2": {
        "type": "text",
        "label": "<p>Correo electrónico</p>",
        "required": true
      },
      "textarea_1": {
        "type": "file",
        "label": "<p>Archvio de prueba</p>",
        "required": true,
        "extras": {
          "accept": "pdf,png,doc,docx",
          "max_size": "5",
          "multiple": true
        }
      },
      "field_3": {
        "type": "select",
        "label": "<p>Opciones</p>",
        "required": true,
        "extras": {
          "options": {
            "opcion_1": "Opción 1",
            "opcion_2": "Opción 2",
            "opcion_three": "Opción 3"
          },
          "select_default": "--Selecciona una opción",
          "div_class": "clase-personalizada"
        }
      },
      "field_4": {
        "type": "checkbox",
        "label": "<p>Seleccionar terminos a aceptar</p>",
        "required": true,
        "extras": {
          "options": {
            "true_cookies": "Aceptar cookies",
            "aceptar_tyc": "Aceptar TyC"
          },
          "is_tos": false
        }
      },
      "field_5": {
        "type": "checkbox",
        "label": "<p>Aceptar el tratamiento de datos personales</p>",
        "required": true,
        "extras": {
          "is_tos": true
        }
      },
      "textarea_2": {
        "type": "textarea",
        "label": "<p>COmentarios adicionales</p>",
        "required": false,
        "extras": {
          "value": "Déjanos un mensaje",
          "rows": "3",
          "cols": "8"
        }
      },
      "field_6": {
        "type": "number",
        "label": "<p>Teléfono de contácto</p>",
        "required": true
      },
      "field_7": {
        "type": "time",
        "label": "<p>Fecha de nacimiento</p>",
        "required": false
      }
    },
    "submit_btn": {
      "submit_type": "send",
      "label": "Probar envío"
    },
    "settings": {
      "no_pre_footer": false,
      "titulo": "Prueba",
      "email_list": "tempcorreo@gmail.com,correo1@test.co",
      "db_viewer": []
    }
  }
]
